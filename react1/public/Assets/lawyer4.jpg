//IE11 START
function composedPath (el) {
var path = [];
while (el) {
path.push(el);
if (el.tagName === 'HTML') {
path.push(document);
path.push(window);
return path;
}
el = el.parentElement;
}
}
if (!Element.prototype.matches) 
Element.prototype.matches = Element.prototype.msMatchesSelector || Element.prototype.webkitMatchesSelector;
if (!Element.prototype.closest) {
Element.prototype.closest = function(s) {
var el = this;
do {
if (Element.prototype.matches.call(el, s)) return el;
el = el.parentElement || el.parentNode;
} while (el !== null && el.nodeType === 1);
return null;
};
}
//IE11 END
configuration.click(configuration.find(".menuMobile .subnonevisible"), function(e){
e.stopImmediatePropagation();
var path = e.path || (e.composedPath && e.composedPath()) || composedPath(e.target);
if(path[0] && path[0].nodeName=="A" && !path[0].classList.contains("notlink")) return;
cleanEvents(this);
if(configuration.findUnique(".menuBar",this) && !this.closest(".menuBarOpened")) return false;
if(this.classList.contains("subnonevisible")){
this.classList.add("subvisible");
this.classList.remove("subnonevisible");
}else{
this.classList.add("subnonevisible");
this.classList.remove("subvisible");
this.classList.remove("leftPos");
}
if(configuration.findUnique(".sublvl3",this) && configuration.findUnique(".sublvl3",this).getBoundingClientRect().left<0){
this.classList.add("leftPos");
}
});
configuration.click(configuration.find(".related.first"), function(e){
e.stopImmediatePropagation();
if(this.classList.contains("closed")){
this.classList.remove("closed");
}else{
this.classList.add("closed");
}
});
var container = document.getElementById("containerMenu");
var selected;
configuration.click(blockLvl1MoreButton, function(e){
populateContainer(e,container,blockLvl1MoreButton,["majorBoxMenu"], ["sub3"], true);
});
configuration.click(blockLvl2MoreButton, function(e){
populateContainer(e,container,blockLvl2MoreButton,["majorBoxMenu"], ["sub3"], true);
});
configuration.click(blockLvl3, function(e){
populateContainer(e,container,this,["majorBoxMenu","sub3"], [], false);
});
function populateContainer(e,container,el,className, toBeRemoved, isMajor){
e.stopPropagation();
if(container.classList.contains("visible") && selected!=el){
container.classList.remove("visible");
for(var k=0;k<className.length;k++)
container.classList.remove(className[k]);
for(var k=0;k<toBeRemoved.length;k++)
container.classList.remove(toBeRemoved[k]);
container.innerHTML="";
